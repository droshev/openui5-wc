{
    "name": "openui5-basic-template-app",
    "version": "0.1.0",
    "description": "Best-practice starting point for building a freestyle app",
    "private": true,
    "scripts": {
        "start": "ui5 serve",
        "lint": "eslint webapp",
        "karma": "karma start",
        "karma-ci": "karma start karma-ci.conf.js",
        "watch": "start-server-and-test start http://localhost:8080 karma",
        "test": "npm run lint && rimraf coverage && start-server-and-test start http://localhost:8080 karma-ci",
        "build": "rimraf dist && ui5 build --a"
    },
    "dependencies": {
        "@openui5/sap.m": "^1.61.0",
        "@openui5/sap.ui.core": "^1.61.0",
        "@openui5/sap.ui.layout": "^1.61.0",
        "@openui5/themelib_sap_belize": "^1.61.0"
    },
    "devDependencies": {
        "@ui5/cli": "^0.2.7",
        "eslint": "^5.10.0",
        "karma": "^3.1.4",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-openui5": "^0.2.3",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-qunit": "^2.1.0",
        "qunitjs": "^2.4.1",
        "rimraf": "^2.6.2",
        "start-server-and-test": "^1.7.11"
    }
}
